[application]

config/name="Jackie Game Gam"
run/main_scene="res://Data/BootLoader/BootLoader.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio_bus_layout.tres"

[autoload]

Game="*res://Data/Game.gd"
Panku="*res://addons/panku_console/console.tscn"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/mode=3
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/panku_console/plugin.cfg")

[file_customization]

